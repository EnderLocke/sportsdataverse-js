{
  "name": "sportsdataverse",
  "version": "1.0.22",
  "description": "Retrieves sports data from a popular sports website as well as from the ncaa website, with support for men's and women's college basketball, NBA, WNBA, NFL, NHL and CFB.",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "chai": "^4.3.0",
    "cheerio": "^1.0.0-rc.5",
    "tabletojson": "2.0.4"
  },
  "devDependencies": {
    "mocha": "^8.3.0"
  },
  "scripts": {
    "test": "mocha --timeout 10000",
    "start": "node server.js",
    "document": "jsdoc -c conf.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saiemgilani/sportsdataverse.git"
  },
