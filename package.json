{
  "name": "sportsdataverse",
  "version": "1.2.2",
  "description": "Node.js client which retrieves sports data from the ESPN API as well as from the NCAA website, with support for NBA, NFL, NHL, WNBA, men's and women's college basketball, and college football.",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "chai": "^4.3.0",
    "cheerio": "^1.0.0-rc.10",
    "decode-html": "^2.0.0",
    "tabletojson": "^2.0.6"
  },
  "devDependencies": {
    "docdash": "latest",
    "jsdoc": "^3.6.10",
    "jsdoc-to-markdown": "^7.1.0",
    "mocha": "^9.2.1",
    "underscore": ">=1.12.1"
  },
  "scripts": {
    "test": "mocha --timeout 10000",
    "start": "node server.js",
    "document": "npm run md-all && npm run md-readme && npm run readme",
    "jsdoc-build": "jsdoc app -r -c conf.json -d docs",
    "jsdoc2md": "node_modules/.bin/jsdoc2md",
    "readme": "jsdoc2md -t md/README.hbs app/services/cfb.service.js app/services/mbb.service.js app/services/nba.service.js app/services/ncaa.service.js app/services/nfl.service.js app/services/nhl.service.js app/services/wbb.service.js app/services/wnba.service.js > README.md",
    "md-readme": "jsdoc2md -t md/intro.hbs app/services/cfb.service.js app/services/mbb.service.js app/services/nba.service.js app/services/ncaa.service.js app/services/nfl.service.js app/services/nhl.service.js app/services/wbb.service.js app/services/wnba.service.js > docs2/docs/intro.md",
    "md-cfb": "jsdoc2md app/services/cfb.service.js > docs2/docs/cfb.md",
    "md-mbb": "jsdoc2md app/services/mbb.service.js > docs2/docs/mbb.md",
    "md-nba": "jsdoc2md app/services/nba.service.js > docs2/docs/nba.md",
    "md-ncaa": "jsdoc2md app/services/ncaa.service.js > docs2/docs/ncaa.md",
    "md-nfl": "jsdoc2md app/services/nfl.service.js > docs2/docs/nfl.md",
    "md-nhl": "jsdoc2md app/services/nhl.service.js > docs2/docs/nhl.md",
    "md-wbb": "jsdoc2md app/services/wbb.service.js > docs2/docs/wbb.md",
    "md-wnba": "jsdoc2md app/services/wnba.service.js > docs2/docs/wnba.md",
    "md-all": "npm run md-cfb && npm run md-mbb && npm run md-nba && npm run md-ncaa && npm run md-nfl && npm run md-nhl && npm run md-wbb && npm run md-wnba"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saiemgilani/sportsdataverse-js.git"
  },
  "keywords": [
    "sports",
    "nba",
    "nfl",
    "nhl",
    "wnba",
    "cbb",
    "wbb",
    "ncaa",
    "espn",
    "basketball",
    "hoops",
    "college-basketball",
    "water-polo-men",
    "water-polo-women",
    "men's-water-polo",
