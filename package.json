    "soccer-women",
    "mens-ice-hockey",
    "women's-ice-hockey",
    "field-hockey",
    "men's-ice-hockey",
    "women's-ice-hockey",
    "ice-hockey-men",
    "ice-hockey-women",
    "baseball",
    "softball",
    "men's-volleyball",
    "women's-volleyball",
    "volleyball-men",
    "volleyball-women",
    "beach-volleyball",
